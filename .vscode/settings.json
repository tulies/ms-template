{
    "editor.formatOnSave": true,
    "eslint.validate": [
        "vue",
        "html",
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact"
    ],
    "files.associations": {
        "*.js": "javascriptreact"
    },
    "eslint.alwaysShowStatus": true,
    "auto-rename-tag.activationOnLanguage": [
        "*"
    ],
    "path-intellisense.mappings": {
        "@": "${workspaceRoot}/src"
    }
}